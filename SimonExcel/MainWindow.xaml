        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Margin="74,126,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Margin="74,177,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Margin="74,215,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox5" HorizontalAlignment="Left" Margin="74,254,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Grid>
            <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="284" Margin="233,44,0,0" VerticalAlignment="Top" Width="495"/>
        </Grid>

    </Grid>
</Window>
